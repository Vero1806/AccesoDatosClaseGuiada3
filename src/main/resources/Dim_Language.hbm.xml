<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="org.example.Dim_Language" table="dim_language">
        <meta attribute="class-description">
            This class contains the language dimension
        </meta>

        <id name="LanguageCode" type="string" column="LanguageCode">
            <generator class="assigned"/>
        </id>
        <property name="Language" column="Language" type="string"/>
        <property name="created_dt" column="created_dt" type="LocalDateTime"/>
        <property name="updated_dt" column="updated_dt" type="LocalDateTime"/>

    </class>
</hibernate-mapping>